html
  head
    title finedust
  body
    h1 #{city}의 현재 미세먼지 농도
    div 
    each item in items 
      ul
        li #{item.cityName}
        li 측정일 #{item.dataTime}
        if item.pm10Value  == '-'
          li 미세먼지 측정 결과 없음
        else
          li 미세먼지 #{item.pm10Value}
          - var finedust_10 = parseInt(item.pm10Value) // 미세먼지 등급
          if finedust_10 > 150
            li 미세먼지 농도 매우나쁨
          else if finedust_10 > 80
            li 미세먼지 농도 나쁨
          else if finedust_10 > 30
            li 미세먼지 농도 보통
          else 
            li 미세먼지 농도 좋음
        if item.pm25Value  == '-'
          li 초미세먼지 측정 결과 없음
        else
          li 초미세먼지 #{item.pm25Value}
          - var finedust_25 = parseInt(item.pm25Value) // 초미세먼지 등급
          if finedust_25 > 100
            li 초미세먼지 농도 매우나쁨
          else if finedust_25 > 50
            li 초미세먼지 농도 나쁨
          else if finedust_25 > 15
            li 초미세먼지 농도 보통
          else 
            li 초미세먼지 농도 좋음
      


